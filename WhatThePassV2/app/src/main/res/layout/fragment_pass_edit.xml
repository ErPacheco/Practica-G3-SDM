<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.passAndFiles.PassEditFragment" >

    <TextView
        android:id="@+id/passwordDetailTITLE2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="104dp"
        android:text="@string/edit_input_password"
        android:textColor="@color/primary_color"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/inputNameLabel2"
        android:layout_width="316dp"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:text="@string/input_name_label"
        android:textColor="@color/third_color"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordDetailTITLE2"
        tools:ignore="SmallSp"
        android:labelFor="@+id/titleDetail2" />

    <EditText
        android:id="@+id/usernameDetail2"
        android:layout_width="316dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/bottom_border"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputUsernameLabel2"
        android:autofillHints="Default field"
        android:inputType="text" />

    <TextView
        android:id="@+id/inputURILabel2"
        android:layout_width="316dp"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:text="@string/uri"
        android:textColor="@color/third_color"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordDetailInput2"
        tools:ignore="SmallSp"
        android:labelFor="@+id/URIDetail2" />

    <EditText
        android:id="@+id/URIDetail2"
        android:layout_width="316dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/bottom_border"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputURILabel2"
        android:autofillHints="Default field"
        android:inputType="textUri" />

    <TextView
        android:id="@+id/inputUsernameLabel2"
        android:layout_width="316dp"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:text="@string/input_username"
        android:textColor="@color/third_color"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailDetail2"
        tools:ignore="SmallSp"
        android:labelFor="@id/usernameDetail2" />

    <EditText
        android:id="@+id/titleDetail2"
        android:layout_width="316dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/bottom_border"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputNameLabel2"
        android:autofillHints="Default field"
        android:inputType="text" />

    <TextView
        android:id="@+id/inputEmailUsernameLabel2"
        android:layout_width="316dp"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:text="@string/input_email"
        android:textColor="@color/third_color"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleDetail2"
        tools:ignore="SmallSp"
        android:labelFor="@id/emailDetail2"/>

    <EditText
        android:id="@+id/emailDetail2"
        android:layout_width="316dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/bottom_border"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputEmailUsernameLabel2"
        android:autofillHints="Default field"
        android:inputType="textWebEmailAddress" />

    <TextView
        android:id="@+id/inputPasswordLabel2"
        android:layout_width="316dp"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:text="@string/input_password"
        android:textColor="@color/third_color"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usernameDetail2"
        tools:ignore="SmallSp"
        android:labelFor="@id/passwordDetailInput2" />

    <EditText
        android:id="@+id/passwordDetailInput2"
        android:layout_width="316dp"
        android:layout_height="30dp"
        android:layout_marginTop="4dp"
        android:autofillHints="Default field"
        android:background="@drawable/bottom_border"
        android:inputType="textPassword"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputPasswordLabel2" />

    <ImageButton
        android:id="@+id/viewButton3"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="34dp"
        android:layout_height="23dp"
        android:layout_marginTop="425dp"
        android:background="#00FFFFFF"
        android:clickable="true"
        android:contentDescription="@string/viewbutton"
        android:outlineProvider="none"
        android:src="@drawable/ic_baseline_remove_red_eye_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.891"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/saveChangesButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="176dp"
        android:text="@string/save_changes"
        android:backgroundTint="@color/primary_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.244"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/cancelChangesButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="176dp"
        android:text="@string/cancel"
        android:backgroundTint="@color/third_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/saveChangesButton" />

</androidx.constraintlayout.widget.ConstraintLayout>